{
  "name": "reason-react-parcel-ssr",
  "version": "0.1.0",
  "scripts": {
    "start": "NODE_ENV=production node dist/server",
    "predev": "node tools/clean.js",
    "dev": "concurrently \"npm run watch:bsb\" \"npm run watch:client\" \"npm run watch:serve\" ",
    "watch:bsb": "bsb -make-world -w",
    "watch:client": "parcel watch src/client.bs.js -o client -d dist/public/assets",
    "watch:serve": "NODE_ENV=development nodemon -q -e js -d 0.5 -w src lib/js/src/server/server.bs.js",
    "build": "npm run build:bsb && npm run build:client && npm run build:server",
    "build:bsb": "bsb -make-world",
    "build:client": "parcel build src/client.bs.js -o client -d dist/public/assets --public-url /assets",
    "build:server": "BABEL_ENV=server parcel build lib/js/src/server/server.bs.js -o index -d dist/server --target=node",
    "prebuild": "node tools/clean.js",
    "postbuild": "node tools/copy.js",
    "tools:copy": "node tools/copy.js",
    "tools:move": "node tools/move.js",
    "tools:clean": "node tools/clean.js",
    "clean:bsb": "bsb -clean-world",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "BuckleScript"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "apollo-cache-inmemory": "^1.1.12",
    "apollo-client": "^2.2.8",
    "apollo-link": "^1.2.2",
    "apollo-link-context": "^1.0.8",
    "apollo-link-error": "^1.0.9",
    "apollo-link-http": "^1.5.4",
    "bs-css": "^6.6.0",
    "bs-dynamic-import": "^0.3.0",
    "bs-express": "^0.7.0",
    "dotenv": "^5.0.1",
    "express": "^4.16.3",
    "graphql": "^0.13.2",
    "graphql-tag": "^2.8.0",
    "react": "^16.3.2",
    "react-apollo": "^2.1.3",
    "react-dom": "^16.3.2",
    "react-helmet": "^5.2.0",
    "react-helmet-async": "^0.0.5",
    "react-hot-loader": "^4.1.0",
    "reason-apollo": "https://github.com/apollographql/reason-apollo.git#feature/react-apollo-2.1",
    "reason-loadable": "^0.4.0",
    "reason-react": "^0.3.4"
  },
  "devDependencies": {
    "@babel/plugin-syntax-dynamic-import": "^7.0.0-beta.44",
    "babel-plugin-dynamic-import-node": "^1.2.0",
    "bs-platform": "^3.0.0",
    "concurrently": "^3.5.1",
    "fs-extra": "^5.0.0",
    "graphql_ppx": "^0.2.1",
    "nodemon": "^1.17.3",
    "parcel-bundler": "^1.7.0"
  }
}